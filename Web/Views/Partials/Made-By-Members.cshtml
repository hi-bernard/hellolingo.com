			<div class="made-by-members">@StringsFoundation.BuiltByMembers</div>
			<div class="active-contribution">@FeatureInYourLang.ActivelyContributing</div>
			<div class="thanks-to-them">@FeatureInYourLang.ThanksToThem</div>
		</div>

		<!-- ============ Sign Up Block =============-->

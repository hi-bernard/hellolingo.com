@using System.Threading
@Styles.Render("~/bundles/made-by-members-css")

<div id="glassReflection"></div>

<div id="made-by-member">

	<!-- ============ LANGUAGE LEARNING NEED HOME =============-->
	<div class="main-block">

		<!-- ============ What is this about? =============-->

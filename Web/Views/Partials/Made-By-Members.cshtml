				<tr><td>Čeština</td><td>   <div id="czech-republic" class="flag" ng-repeat="i in [1,2,3]"></div> 30%</td></tr>
				<tr><td>Tiếng Việt</td><td><div id="vietnam" class="flag" ng-repeat="i in [1,2,3]"></div> 30%</td></tr>
				<tr><td>Polski</td><td>    <div id="poland" class="flag" ng-repeat="i in [1,2]"></div> 20%</td></tr>
			</table>
		</div>

	</div>


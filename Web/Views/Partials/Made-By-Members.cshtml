				<tr><td>Italiano</td><td>  <div id="italy" class="flag" ng-repeat="i in [1,2,3,4,5,6,7,8,9]"></div> 90%</td></tr>
				<tr><td>Português</td><td> <div id="portugal" class="flag" ng-repeat="i in [1,2,3,4,5,6,7,8,9]"></div> 90%</td></tr>
				<tr><td>עברית</td><td>     <div id="israel" class="flag" ng-repeat="i in [1,2,3,4,5,6,7,8]"></div> 80%</td></tr>

<configuration>
  
	<configSections>
    <section name="entityFramework" type="System.Data.Entity.Internal.ConfigFile.EntityFrameworkSection, EntityFramework, Version=6.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
	</configSections>
	
	<connectionStrings>
		<add name="HelloLingoEntities" connectionString="metadata=res://*/DataAccess.SqlModel.csdl|res://*/DataAccess.SqlModel.ssdl|res://*/DataAccess.SqlModel.msl;provider=System.Data.SqlClient;provider connection string=&quot;data source=(LocalDb)\MSSQLLocalDB;initial catalog=HelloLingoSQL;integrated security=True;multipleactiveresultsets=True;application name=EntityFramework&quot;" providerName="System.Data.EntityClient" />
		<!-- HangFire requires DB_Owner permissions to create the tables the first time. Staging/Prod restrict the IIS_AppPool identity to less than that. You'll either have to create the table manually, or allow db_owner temporarily -->
		<add name="HangFireConnectionString" connectionString="data source=(LocalDb)\MSSQLLocalDB;initial catalog=HelloLingoSQL;integrated security=True;multipleactiveresultsets=True;" providerName="System.Data.SqlClient" />
	</connectionStrings>
	
	
	<appSettings>
		<add key="webpages:Version" value="3.0.0.0" />
		<add key="webpages:Enabled" value="false" />
		<add key="ClientValidationEnabled" value="true" />
		<add key="UnobtrusiveJavaScriptEnabled" value="true" />
		
		<add key="AdminMail" value="admin@hellolingo.com" />

		<add key="xirsys:ident" value="hellolingo" />
		<add key="xirsys:secret" value="edc3ca38-f609-11e5-95bb-d38f116e02f9" />
		<add key="xirsys:domain" value="www.hellolingo.com" />
		<add key="xirsys:application" value="default" />
		<add key="xirsys:room" value="development" />

		<!-- If you enable this, you also need to uncomment the "textWriterTraceListener" below -->
		<add key="log4net.Internal.Debug" value="false" />
		<add key="EmailLogPath" value="../../Logs/EmailsOut" />
		
		<!--Email sending quotas-->
		<add key="EmailValidationByEmail" value="5" />
		<add key="EmailValidationByUserId" value="10" />
		<add key="EmailValidationsTotal" value="150" />
		<add key="TotalByEmail" value="20" />
		<add key="TextChatAlertsTotal" value="100" />
		<add key="EmailsTotal" value="1000" />
		
	</appSettings>
	
	<system.web>
    <authentication mode="None" />
    <compilation debug="true" targetFramework="4.6" />
    <httpRuntime targetFramework="4.6" />
	</system.web>
	
	<system.webServer>
		
		<modules><remove name="FormsAuthentication" /></modules>
		
		<handlers>
      <remove name="ExtensionlessUrlHandler-Integrated-4.0" />
      <remove name="OPTIONSVerbHandler" />
      <remove name="TRACEVerbHandler" />
      <add name="ExtensionlessUrlHandler-Integrated-4.0" path="*." verb="*" type="System.Web.Handlers.TransferRequestHandler" preCondition="integratedMode,runtimeVersionv4.0" />
    </handlers>

		<staticContent>
			<remove fileExtension=".woff2" />
			<mimeMap fileExtension=".woff2" mimeType="application/font-woff2" />
		</staticContent>

	</system.webServer>


